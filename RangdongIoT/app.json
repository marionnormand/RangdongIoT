{
  "expo": {
    "name": "RangdongIot",
    "slug": "RangdongIot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_rangDong.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/rang-dong-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "marion"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rang-dong-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "rang.dong"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a0c2789c-af37-4ff4-9467-9cf3e67fff8f"
      }
    }
  }
}
